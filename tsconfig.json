{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "rn-yurberz-packages": ["."],
      "rn-yurberz-packages/*": ["./*"]
    }
  },
  "include": ["index.ts"],
  "exclude": ["node_modules", "dist"]
}
